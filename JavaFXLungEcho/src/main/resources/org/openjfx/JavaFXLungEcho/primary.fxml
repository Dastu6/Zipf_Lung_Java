<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.JavaFXLungEcho.PrimaryController">
   <children>
      <Pane prefHeight="714.0" prefWidth="1240.0">
         <children>
            <ImageView fx:id="imageContainer1" fitHeight="228.0" fitWidth="369.0" layoutX="734.0" layoutY="206.0" onMouseClicked="#imageClicked2" pickOnBounds="true" preserveRatio="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></ImageView>
            <ImageView fx:id="imageContainer" fitHeight="179.0" fitWidth="402.0" layoutX="218.0" layoutY="230.0" onMouseClicked="#imageClicked" pickOnBounds="true" preserveRatio="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></ImageView>
            <Button fx:id="imageSelectionButton" layoutX="554.0" layoutY="549.0" onMouseClicked="#switchImage" text="Sélectionner cette image">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="traitementImageButton" layoutX="582.0" layoutY="505.0" onMouseClicked="#cutImage" text="Traiter l'image">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Slider fx:id="sliderImage" blockIncrement="1.0" layoutX="506.0" layoutY="434.0" majorTickUnit="1.0" max="1000.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="38.0" prefWidth="229.0" showTickMarks="true">
               <cursor>
                  <Cursor fx:constant="H_RESIZE" />
               </cursor>
            </Slider>
            <ListView fx:id="listview" layoutX="419.0" layoutY="131.0" onMouseClicked="#clickOnlistview" prefHeight="188.0" prefWidth="402.0" />
            <Button fx:id="selectDirButton" layoutX="455.0" layoutY="52.0" onMouseClicked="#buttonCall" text="Sélectionner un dossier">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label fx:id="labelList" alignment="CENTER" contentDisplay="CENTER" layoutX="557.0" layoutY="90.0" text="Liste des fichiers ouvrables" />
            <Button fx:id="changeImage" layoutX="248.0" layoutY="131.0" mnemonicParsing="false" onMouseClicked="#changerImage" text="Changer d'image">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <TextField fx:id="sliderTextField" layoutX="569.0" layoutY="472.0" onAction="#submitValue" prefHeight="25.0" prefWidth="51.0">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <Label fx:id="labelSlider" layoutX="627.0" layoutY="476.0" text="Label" />
            <Button fx:id="selectFileButton" layoutX="629.0" layoutY="52.0" mnemonicParsing="false" onMouseClicked="#selectFileButtonAction" text="Sélectionner une image">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <CheckBox fx:id="darkTheme" layoutX="212.0" layoutY="332.0" mnemonicParsing="false" onAction="#changeTheme" selected="true" text="Dark Theme" />
         </children>
      </Pane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
